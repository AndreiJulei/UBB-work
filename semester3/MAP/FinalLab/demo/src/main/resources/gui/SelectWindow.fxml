<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="15.0" prefHeight="400.0" prefWidth="600.0" 
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="gui.SelectWindowController">
    <children>
        <Label text="Select a Program to Execute" style="-fx-font-size: 16px; -fx-font-weight: bold;" />
        
        <ListView fx:id="programListView" VBox.vgrow="ALWAYS" />
        
        <Button fx:id="selectButton" mnemonicParsing="false" onAction="#handleSelect" 
                text="Load Program" prefWidth="150.0" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>